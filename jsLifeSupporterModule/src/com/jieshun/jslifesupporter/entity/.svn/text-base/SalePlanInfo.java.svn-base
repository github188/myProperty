package com.jieshun.jslifesupporter.entity;

import java.io.Serializable;

public class SalePlanInfo implements Serializable {

	/**
	 * 
	 */
	private static final long serialVersionUID = -2335524358294011166L;
	//方案Id
	private String salePlanId;
	//策略Id
	private String strategyId;
	//方案名称
	private String planNmae;
	//策略名称
	private String tacticsName;
	//优惠方案
	private int tacticsWay;
	//优惠数额
	private double amount;
	public String getSalePlanId() {
		return salePlanId;
	}
	public void setSalePlanId(String salePlanId) {
		this.salePlanId = salePlanId;
	}
	public String getStrategyId() {
		return strategyId;
	}
	public void setStrategyId(String strategyId) {
		this.strategyId = strategyId;
	}
	public String getPlanNmae() {
		return planNmae;
	}
	public void setPlanNmae(String planNmae) {
		this.planNmae = planNmae;
	}
	public String getTacticsName() {
		return tacticsName;
	}
	public void setTacticsName(String tacticsName) {
		this.tacticsName = tacticsName;
	}
	public int getTacticsWay() {
		return tacticsWay;
	}
	public void setTacticsWay(int tacticsWay) {
		this.tacticsWay = tacticsWay;
	}
	public double getAmount() {
		return amount;
	}
	public void setAmount(double amount) {
		this.amount = amount;
	}
	
	
}
